<h1> List: </h1>
<table>
<% $results.each do |r| %>
        <tr>
                <td> <%= r.sortname  %> </td>
                <td> <%= r.dob       %> </td>
                <td> <%= r.sex       %> </td>
                <td> <%= r.pat_ssn   %> </td>
                <td> <%= r.addr1     %> </td>
                <td> <%= r.city      %> </td>
                <td> <%= r.state     %> </td>
                <td> <%= r.zip       %> </td>
        </tr>
<% end %>
</table>

